<!DOCTYPE html>
<!--[if IE 9]>         <html class="ie9 no-focus" lang="en"> <![endif]-->
<!--[if gt IE 9]><!--> <html class="no-focus" lang="en"> <!--<![endif]-->
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Default title{% endblock %} | {{ admin_app_name }}</title>
    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1.0">

    <!-- Icons -->
    <link rel="shortcut icon" href="{{ asset('favicon.ico') }}">
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

    {% block stylesheets %}
        <!-- Web fonts -->
        <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400italic,600,700%7COpen+Sans:300,400,400italic,600,700">

        <!-- Style sheets -->
        <link rel="stylesheet" href="{{ asset('admin/assets/js/plugins/bootstrap-colorpicker/css/bootstrap-colorpicker.min.css') }}">
        <link rel="stylesheet" href="{{ asset('admin/assets/js/plugins/select2/select2.min.css') }}">
        <link rel="stylesheet" href="{{ asset('admin/assets/js/plugins/select2/select2-bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('admin/assets/js/plugins/bootstrap-datetimepicker/bootstrap-datetimepicker.min.css') }}">
        <link rel="stylesheet" href="{{ asset('admin/assets/js/plugins/magnific-popup/magnific-popup.min.css') }}">

        <link rel="stylesheet" href="{{ asset('admin/assets/css/bootstrap.min.css') }}">
        <link rel="stylesheet" id="css-main" href="{{ asset('admin/assets/css/oneui.css') }}">
        <link rel="stylesheet" href="{{ asset('admin/css/main.css') }}">
    {% endblock stylesheets %}

    {% block javascripts_head %}
        <script src="{{ asset('admin/assets/js/core/jquery.min.js') }}"></script>
    {% endblock %}
</head>
<body>
{% block body %}

    <!-- Page Container -->
    <div id="page-container" class="sidebar-l sidebar-o side-scroll header-navbar-fixed">

        {% include 'admin/layout/component/_side_overlay.html.twig' %}

        {% include 'admin/layout/component/_sidebar.html.twig' %}

        {% include 'admin/layout/component/_header.html.twig' %}

        <!-- Main Container -->
        <main id="main-container">
            {% block main_container %}

                {% block page_header %}
                {% endblock page_header %}

                <!-- Page Content -->
                <div class="content">
                    {% block page_content_nav %}{% endblock page_content_nav %}
                    {% block page_content %}{% endblock page_content %}
                </div>
                <!-- END Page Content -->

            {% endblock main_container %}
        </main>
        <!-- END Main Container -->

        {% include 'admin/layout/component/_footer.html.twig' %}

    </div>
    <!-- END Page Container -->

    {% include 'admin/layout/component/_apps_modal.html.twig' %}

{% endblock %}

{% block javascripts %}
    <!-- OneUI Core JS: jQuery, Bootstrap, slimScroll, scrollLock, Appear, CountTo, Placeholder, Cookie and App.js -->
    <script src="{{ asset('admin/assets/js/core/jquery.min.js') }}"></script>
    <script src="{{ asset('admin/assets/js/core/bootstrap.min.js') }}"></script>
    <script src="{{ asset('admin/assets/js/core/jquery.slimscroll.min.js') }}"></script>
    <script src="{{ asset('admin/assets/js/core/jquery.scrollLock.min.js') }}"></script>
    <script src="{{ asset('admin/assets/js/core/jquery.appear.min.js') }}"></script>
    <script src="{{ asset('admin/assets/js/core/jquery.countTo.min.js') }}"></script>
    <script src="{{ asset('admin/assets/js/core/jquery.placeholder.min.js') }}"></script>
    <script src="{{ asset('admin/assets/js/core/js.cookie.min.js') }}"></script>
    <script src="{{ asset('admin/assets/js/app.js') }}"></script>

    {# bootstrap-datetimepicker #}
    <script src="{{ asset('admin/assets/js/plugins/bootstrap-datetimepicker/moment.min.js') }}"></script>
    <script src="{{ asset('admin/assets/js/plugins/bootstrap-datetimepicker/bootstrap-datetimepicker.min.js') }}"></script>
    <script src="{{ asset('admin/assets/js/plugins/bootstrap-datetimepicker/locale/es.js') }}" charset="UTF-8"></script>
    {# bootstrap-notify #}
    <script src="{{ asset('admin/assets/js/plugins/bootstrap-notify/bootstrap-notify.min.js') }}"></script>
    {# bootstrap-colorpicker #}
    <script src="{{ asset('admin/assets/js/plugins/bootstrap-colorpicker/bootstrap-colorpicker.min.js') }}"></script>
    {# select2 #}
    <script src="{{ asset('admin/assets/js/plugins/select2/select2.full.min.js') }}"></script>
    {# Magnific popup #}
    <script src="{{ asset('admin/assets/js/plugins/magnific-popup/magnific-popup.min.js') }}"></script>
    {# APP js #}
    <script src="{{ asset('admin/js/main.js') }}"></script>
    {# FOSJsRoutingBundle #}
    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script>

    {# flash alerts #}
    {{ include('admin/layout/script/_flash_alerts_script.html.twig') }}
    {# modal #}
    {{ include('admin/layout/script/_modal_script.html.twig') }}
{% endblock %}
</body>
</html>